  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\debug\\app.js",
      "runtimeExecutable": "nodemon",
      "restart": true, //코드 변경때 마다 새롭게 재시작 true
      "outFiles": ["${workspaceFolder}/**/*.js"]
    }
  ]
}
